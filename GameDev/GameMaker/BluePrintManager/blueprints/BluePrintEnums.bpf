@blueprint AverageTB:textureBlendingEnum
	@domain type+={average}
	@property calcB=($prevB+$currB)/2
	@property calcG=($prevG+$currG)/2
	@property calcR=($prevR+$currR)/2
	@property value='sum'
@end

@blueprint divideTB:textureBlendingEnum
	@domain type+={divide}
	@property calcB=if($prevB==0,0,$currG/$prevB)
	@property calcG=if($prevG==0,0,$currG/$prevG)
	@property calcR=if($prevR==0,0,$currR/$prevR)
	@property value='divide'
@end

@blueprint multiplyTB:textureBlendingEnum
	@domain type+={multiply}
	@property calcB=$prevB*$currB
	@property calcG=$prevG*$currG
	@property calcR=$prevR*$currR
	@property value='multiply'
@end

@blueprint replaceTB:textureBlendingEnum
	@domain type+={replace}
	@property calcB=$currB
	@property calcG=$currG
	@property calcR=$currR
	@property value='replace'
@end

@blueprint sumTB:textureBlendingEnum
