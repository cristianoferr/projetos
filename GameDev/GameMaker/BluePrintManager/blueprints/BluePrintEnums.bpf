@blueprint niftyEffectType:NiftyEnums
	@domain type+={niftyEffectType}
	@property neverStopRendering=0
	@property neverStopRenderingValue=if($this.neverStopRendering==1,'true','false')
	@property paramsTags={neverStopRendering}
	@replicate params=tagList($this.paramsTags,{enum niftyEffectParam leaf})
	@childproperty params[endSize].value=$this.endSize
	@childproperty params[end].value=$this.end
	@childproperty params[length].value=$this.length
	@childproperty params[neverStopRendering].value=$this.neverStopRenderingValue
	@childproperty params[startDelay].value=$this.startDelay
	@childproperty params[startSize].value=$this.startSize
	@childproperty params[start].value=$this.start
@end

